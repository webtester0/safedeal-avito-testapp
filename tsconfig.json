{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": true,
    "esModuleInterop": true,
    "strict": false,
    "paths": {
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@assets/*": ["src/assets/*"],
    }
  },
  "include": [
    "./src/*", "webpack.config.js"
  ],
  "exclude": [
  ]
}
